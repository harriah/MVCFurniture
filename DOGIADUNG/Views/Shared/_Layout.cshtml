        function addQuantity(className, id) {
            var value = parseInt($("." + className).val());
            if (value < 9999) {
                value = value + 1;
                $("." + className).val(value);
                $.ajax({
                    url: '/Carts/Update',
                    type: 'POST',
                    dataType: 'json',
                    data: {
                        product_id: parseInt(className),
                        quantity: $("." + className).val(),
                        id: id
                    },
                    beforeSend: function () {
                        // Loading('show');
                    },
                    complete: function () {
                        // Loading('hiden');
                    },
                    success: function (response) {
                        if (response.success && response.Data) {
                            $("." + className).text(response.Data.pricestr);
                            var tongtien = parseInt($(".tong-tien-cart-detail").val());
                            var tongtiengupdate = tongtien + response.Data.price;
                            $(".tong-tien-cart-detail").val(tongtiengupdate);
                            $(".tong-tien-cart").text(tongtiengupdate);
                            
                        }
                        //else{
                        //    $("#categoryModal").modal("show");
                        //}
                    }
                })
            }


        }

        function minusQuantity(className, id) {
            var value = parseInt($("." + className).val());
            var status = value - 1;;
            if (value < 9999 && value >= 2) {
                $("." + className).val(value - 1);
                
            }
            $.ajax({
                url: '/Carts/Update',
                type: 'POST',
                dataType: 'json',
                data: {
                    product_id: parseInt(className),
                    quantity: $("." + className).val(),
                    id: id
                },
                beforeSend: function () {
                    // Loading('show');
                },
                complete: function () {
                    // Loading('hiden');
                },
                success: function (response) {
                    if (response.success && response.Data) {
                        $("." + className).text(response.Data.pricestr);
                        if(status != 0){
                            var tongtien = parseInt($(".tong-tien-cart-detail").val());
                            var tongtiengupdate = tongtien - response.Data.price;
                            $(".tong-tien-cart-detail").val(tongtiengupdate);
                            $(".tong-tien-cart").text(tongtiengupdate);
                        }
                      
                    }
                    //if (response.success) {
                    //    $("#CartsModal").modal("show");
                    //}
                    //else{
                    //        $("#categoryModal").modal("show");
                    //}
                }
            })

        }
        function deleteCart(id,tag) {
            $.ajax({
                url: '/Carts/Deleted',
                type: 'POST',
                dataType: 'json',
                data: {
                    id: id
                },
                beforeSend: function () {
                    // Loading('show');
                },
                complete: function () {
                    // Loading('hiden');
                },
                success: function (response) {
                    if (response.success) {
                       var quantity = parseInt($(tag).parent().find("#quantity").val());
                       var pricePrudct = parseInt($(tag).parent().find("#priceProduct").val());

                        var tongtien = parseInt($(".tong-tien-cart-detail").val());
                        var tongtiengupdate = tongtien - quantity * pricePrudct;
                        $(".tong-tien-cart-detail").val(tongtiengupdate);
                        $(".tong-tien-cart").text(tongtiengupdate);


                        $(tag).parent().parent().parent().remove();
                        /* $("#CartsModal").modal("show"); */
                    }
                }
            })

        }





